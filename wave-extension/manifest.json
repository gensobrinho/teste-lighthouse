{
  "background": {
    "service_worker": "service_worker.js"
  },
  "commands": {
    "toggle-extension": {
      "description": "Send a 'toggle-extension' event to the extension",
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      }
    }
  },
  "description": "Evaluate web accessibility within your browser.",
  "host_permissions": [
    "file:///*",
    "http://*/*",
    "https://*/*"
  ],
  "key": "",
  "manifest_version": 3,
  "name": "WAVE Evaluation Tool",
  "permissions": [
    "activeTab",
    "contextMenus",
    "scripting"
  ],
  "short_name": "WAVE",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "3.2.7.2",
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "img/*",
        "styles/*",
        "chrome/*",
        "background.js",
        "inject.js",
        "sidebar.html",
        "wave.min.js",
        "sidebar.min.js"
      ]
    }
  ],
  "action": {
    "default_title": "WAVE"
  }
}